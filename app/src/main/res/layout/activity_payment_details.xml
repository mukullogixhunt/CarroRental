        <androidx.cardview.widget.CardView
            android:id="@+id/couponCard"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="@dimen/_8sdp"
            app:cardElevation="@dimen/_10sdp"
            app:cardCornerRadius="@dimen/_10sdp"
            android:backgroundTint="@color/primary_light9">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:padding="@dimen/_5sdp">

                <TextView
                    android:id="@+id/tv_code"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/got_a_promo_code"
                    android:textSize="@dimen/_11ssp"
                    android:textStyle="bold"
                    android:textColor="@color/black"
                    android:layout_marginStart="@dimen/_10sdp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <ImageView
                    android:id="@+id/iv_down"
                    android:layout_width="@dimen/_15sdp"
                    android:layout_height="@dimen/_15sdp"
                    android:src="@drawable/ic_arrow_down"
                    android:layout_marginEnd="@dimen/_10sdp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <ImageView
                    android:id="@+id/iv_up"
                    android:layout_width="@dimen/_15sdp"
                    android:layout_height="@dimen/_15sdp"
                    android:src="@drawable/ic_up_arrow"
                    android:visibility="gone"
                    android:layout_marginEnd="@dimen/_10sdp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <androidx.cardview.widget.CardView
                    android:id="@+id/cv_code"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/_7sdp"
                    app:cardCornerRadius="@dimen/_7sdp"
                    android:visibility="gone"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/tv_code">

                    <!-- Container for Applying a Coupon -->
                    <LinearLayout
                        android:id="@+id/layout_apply_coupon"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:visibility="visible"
                        android:orientation="horizontal">

                        <EditText
                            android:id="@+id/et_code"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:background="@color/white"
                            android:hint="@string/enter_coupon_code"
                            android:paddingVertical="@dimen/_7sdp"
                            android:paddingStart="@dimen/_10sdp"
                            android:textSize="@dimen/_10ssp" />

                        <TextView
                            android:id="@+id/btnApplyCoupon"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:background="@color/gray_light"
                            android:gravity="center"
                            android:paddingHorizontal="@dimen/_10sdp"
                            android:text="@string/apply"
                            android:textColor="@color/white"
                            android:textSize="@dimen/_10ssp"
                            android:textStyle="bold" />
                    </LinearLayout>

                    <!-- Container for Applied Coupon State -->
                    <LinearLayout
                        android:id="@+id/layout_applied_coupon"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:visibility="gone"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/tv_applied_coupon_code"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:background="@color/white"
                            android:paddingVertical="@dimen/_7sdp"
                            android:paddingStart="@dimen/_10sdp"
                            android:textColor="@color/green"
                            android:textSize="@dimen/_11ssp"
                            android:textStyle="bold"
                            tools:text="PROMO50 APPLIED" />

                        <TextView
                            android:id="@+id/btnRemoveCoupon"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:background="@color/red"
                            android:gravity="center"
                            android:paddingHorizontal="@dimen/_10sdp"
                            android:text="@string/remove"
                            android:textColor="@color/white"
                            android:textSize="@dimen/_10ssp"
                            android:textStyle="bold" />
                    </LinearLayout>

                </androidx.cardview.widget.CardView>
            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.cardview.widget.CardView>


        <com.google.android.material.button.MaterialButton
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/btnBook"
            android:text="@string/book_now"
            android:textSize="@dimen/_15ssp"
            android:paddingVertical="@dimen/_7sdp"
            app:cornerRadius="@dimen/_8sdp"
            android:layout_margin="@dimen/_10sdp"/>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:visibility="gone"

            android:orientation="horizontal">
            <TextView
                android:id="@+id/tv_inclusions"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:paddingVertical="@dimen/_10sdp"
                android:background="@color/primary_field"
                android:gravity="center"
                android:text="@string/inclusions"
                android:textColor="@color/black"
                android:textSize="@dimen/_13ssp"/>
            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/primary"/>
            <TextView
                android:id="@+id/tv_exclusions"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingVertical="@dimen/_10sdp"
                android:layout_weight="1"
                android:background="@color/white"
                android:gravity="center"
                android:text="@string/exclusions"
                android:textColor="@color/black"
                android:textSize="@dimen/_13ssp"/>
            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/primary"/>
            <TextView
                android:id="@+id/tv_t_c"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingVertical="@dimen/_10sdp"
                android:layout_weight="1"
                android:background="@color/white"
                android:gravity="center"
                android:text="@string/t_c"
                android:textColor="@color/black"
                android:textSize="@dimen/_13ssp"/>

        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
